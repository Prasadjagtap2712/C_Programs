
 // WAP to find the sum of number which are not divisible 3
 

#include<stdio.h>
 void main() {
	 int a ;
	 printf("Enter a number :") ;
	 scanf("%d", &a);
         int sum = 0;
	for (int i = 1; i <= a; i++) {
		if (i%3 != 0) {
			
		          sum = sum + i;
		}
	}
	printf("sum of numbers not divisible by 3: %d\n",sum);
 }



